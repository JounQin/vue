{
  "name": "@rxts/types",
  "version": "0.0.0",
  "description": "TypeScript Type Definitions same as DefinitelyTyped.",
  "repository": "git@github.com:rx-ts/types.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:bundle": "r",
    "build:ts": "tsc -b",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --ext js,md,mdx,ts,tsx -f friendly",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --incremental false --noEmit",
    "postinstall": "t || exit 0",
    "test": "ts-node scripts/dtslint",
    "type-coverage": "type-coverage --cache --ignore-catch --detail --strict"
  },
  "devDependencies": {
    "@1stg/babel-preset": "^0.7.10",
    "@1stg/commitlint-config": "^0.1.2",
    "@1stg/eslint-config": "^0.13.5",
    "@1stg/husky-config": "^0.3.2",
    "@1stg/lint-staged": "^0.8.12",
    "@1stg/prettier-config": "^0.4.3",
    "@1stg/remark-config": "^0.2.2",
    "@1stg/tsconfig": "^0.6.1",
    "@1stg/tslint-config": "^0.4.4",
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.2.0",
    "@pkgr/rollup": "^0.6.4",
    "@types/node": "^12.12.0",
    "dtslint": "^0.9.9",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "lerna": "^3.18.3",
    "lerna-changelog": "^0.8.2",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "type-coverage": "^2.3.0",
    "typescript": "^3.6.4",
    "vue": "^2.6.10"
  },
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "prettier": "@1stg/prettier-config",
  "renovate": {
    "extends": "@1stg"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
