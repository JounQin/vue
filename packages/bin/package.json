  "description": "Link @d-ts packages to @types automatically",
  "repository": "git+https://github.com/rx-ts/types.git",
  "homepage": "https://github.com/rx-ts/types/tree/master/packages/bin",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "bin": {
